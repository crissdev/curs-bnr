  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@vitest/coverage-c8": "^0.31.1",
    "chalk": "^4.1.2",
    "microbundle": "^0.15.1",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  }
